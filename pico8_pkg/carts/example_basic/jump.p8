version 42
__lua__
a={y=100,dy=0}
grvt=0.8
function _update()
	if(btn(🅾️)) then	a.dy=-4 end
	a.dy += grvt
	a.y += a.dy
	if(a.y>100) then a.y=100 end
end

function _draw()
	cls()
	circ(64,a.y,4,3)
end
